# ML App Environment Configuration
# Copy this file to .env and update with your values

# GCP Configuration
PROJECT_ID=your-gcp-project-id
CLUSTER_NAME=ml-app-cluster
ZONE=us-central1-a
REGION=us-central1

# Domain Configuration (for Ingress)
DOMAIN=ml-app.example.com

# Application Configuration
REACT_APP_API_URL=http://ml-backend-service:5000

# Storage Configuration
MODEL_STORAGE_SIZE=10Gi
TRAINING_DATA_SIZE=5Gi
IMAGE_DATA_SIZE=20Gi

# Security Configuration
# Generate with: cat gcp-service-account-key.json | base64 -w 0
GCP_CREDENTIALS_BASE64=
